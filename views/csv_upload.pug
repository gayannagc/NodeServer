extends layout

block content
    .container
        div.row
            div.col-md-6.m-auto
                h1.text-center.display-4.my-4 #{title}
                form(action='/roadsign/upload/csv', method='POST', enctype="multipart/form-data" )
                    .custom-file.mb-3
                        #form-group
                            label Upload File:
                            input.form-control(name= 'myFile' ,type="file")
                    br
                    input.btn.btn-primary.btn-block.my-4(type='submit', value='Upload a file')
